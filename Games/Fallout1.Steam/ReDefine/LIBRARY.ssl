procedure start;
procedure spatial_p_proc;


procedure start
begin
    if (script_action == spatial_proc) then begin
        call spatial_p_proc();
    end
end

procedure spatial_p_proc
begin
    if (source_obj == dude_obj) then begin
        if (local_var(0) == 0) then begin
            set_local_var(0, 1);
            display_mstr(100);
        end
    end
end
