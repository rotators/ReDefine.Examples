import variable Use_Manhole_Top;

procedure start;
procedure use_p_proc;


procedure start
begin
    if (script_action == use_proc) then begin
        call use_p_proc();
    end
end

procedure use_p_proc
begin
    move_to(source_obj, 17089, 1);
    if (source_obj == dude_obj) then begin
        anim(source_obj, 0, 0);
    end
end
