procedure start;


procedure start
begin
    if ((script_action == 21) or (script_action == 3)) then begin
        script_overrides;
        display_msg(message_str(428, 100));
    end
    else begin
        if (script_action == 8) then begin
            script_overrides;
            display_msg(message_str(428, 101));
        end
    end
end

