procedure start;


procedure start
begin
    if (script_action == 2) then begin
        if (source_obj == dude_obj) then begin
            if (local_var(0) != 1) then begin
                if (is_success(do_check(dude_obj, 1, 0))) then begin
                    display_msg(message_str(97, 100));
                end
                set_local_var(0, 1);
            end
        end
    end
end

