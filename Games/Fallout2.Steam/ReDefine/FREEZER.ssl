procedure start;


procedure start
begin
    if (script_action == look_at_proc) then begin
        script_overrides;
        display_msg(message_str(SCRIPT_RNDCOAST, 100));
    end
    else begin
        if (script_action == description_proc) then begin
            script_overrides;
            display_msg(message_str(SCRIPT_RNDCOAST, 101));
        end
    end
end
