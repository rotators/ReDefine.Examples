procedure PayDude
begin
    if (item_caps_total(dude_obj) >= Heal_Cost) then begin
        item_caps_adjust(dude_obj, -Heal_Cost);
        critter_heal(dude_obj, get_critter_stat(dude_obj, STAT_max_hp) - get_critter_stat(dude_obj, STAT_current_hp));
        critter_injure(dude_obj, 4 bwor 8 bwor 16 bwor 32 bwor 8388608);
        gfade_out(600);
        if (Heal_Cost == 300) then begin
            game_time_advance(game_ticks(8 * (60 * (60 * 10))));
        end
        else begin
            game_time_advance(game_ticks(2 * (60 * (60 * 10))));
        end
        gfade_in(600);
        Reply(315);
        giq_option(4, NAME, 202, Node001, NEUTRAL_REACTION);
        giq_option(4, NAME, 203, Node999, NEUTRAL_REACTION);
    end
    else begin
        call Node017();
    end
end

