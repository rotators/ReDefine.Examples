procedure critter_p_proc
begin
    if ((local_var(11) == 1) and (tile_num(self_obj) == local_var(13)) and (obj_get_rot(self_obj) != local_var(14)) and (anim_busy(self_obj) == 0)) then begin
        if (local_var(12) == 4) then begin
            if (tile_distance(14930, dude_tile) > 10) then begin
                anim(self_obj, 1000, local_var(14));
                float_msg(self_obj, mstr(169), FLOAT_MSG_YELLOW);
            end
            else begin
                anim(self_obj, 1000, rotation_to_tile(tile_num(self_obj), dude_tile));
                call Node020();
            end
        end
        else begin
            anim(self_obj, 1000, local_var(14));
            call SpawnResponse();
            call Node020();
        end
        if (local_var(12) >= 7) then begin
            set_local_var(12, 0);
        end
        else begin
            set_local_var(12, local_var(12) + 1);
        end
        add_timer_event(self_obj, game_ticks(6), 0);
    end
    else begin
        if ((local_var(11) == 1) and (tile_num(self_obj) != local_var(13)) and (anim_busy(self_obj) == 0)) then begin
            animate_move_obj_to_tile(self_obj, local_var(13), 0);
        end
    end
    if (obj_can_see_obj(self_obj, dude_obj)) then begin
        if (local_var(5) == 2) then begin
            set_local_var(5, 1);
            attack(dude_obj);
        end
        else begin
            if ((global_var(GVAR_NAVARRO_BASE_ALERT) > 0) and (local_var(10) == 0)) then begin
                set_local_var(10, 1);
                dialogue_system_enter;
            end
