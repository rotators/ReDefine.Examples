procedure Node1100
begin
    if (dude_charisma == 1) then begin
        if (obj_in_party(self_obj)) then begin
            debug_msg("abandon party: " + self_name);
            party_remove(self_obj);
        end
        if ((obj_pid(self_obj) == 16777380) or (obj_pid(self_obj) == 16777379)) then begin
            debug_msg("start: set_dude_was_married");
            if ((global_var(GVAR_PLAYER_MARRIED) != 0) and ((global_var(GVAR_PLAYER_WAS_MARRIED) != 0) == 0)) then begin
                debug_msg("   GVAR_PLAYER_WAS_MARRIED before == " + global_var(GVAR_PLAYER_WAS_MARRIED));
                set_global_var(GVAR_PLAYER_WAS_MARRIED, global_var(GVAR_PLAYER_MARRIED));
                debug_msg("   GVAR_PLAYER_WAS_MARRIED after == " + global_var(GVAR_PLAYER_WAS_MARRIED));
                set_global_var(GVAR_PLAYER_MARRIED, 0);
            end
            debug_msg("finished: set_dude_was_married");
        end
        if ((local_var(10) != -1) and (has_trait(TRAIT_OBJECT, self_obj, OBJECT_TEAM_NUM) == 0)) then begin
            critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, local_var(10));
            set_local_var(10, -1);
        end
        debug_msg("" + self_name + " has abandoned the party");
        Reply(278);
    end
    else begin
        if (global_var(GVAR_REPUTATION_SLAVER) == 1) then begin
            Reply(277);
        end
        else begin
            if ((global_var(GVAR_CHILDKILLER_REPUTATION) >= 2) or (global_var(GVAR_PLAYER_REPUTATION) < -100)) then begin
                Reply(277);
            end
            else begin
                if (((party_member_count(0) - 1) >= (floor(dude_charisma / 2) + has_trait(TRAIT_PERK, dude_obj, PERK_magnetic_personality_perk))) or ((party_member_count(0) - 1) >= 5)) then begin
                    Reply(276);
                end
                else begin
                    Perform_Party_Add := 1;
                    Reply(289);
                end
            end
        end
    end
    if ((global_var(GVAR_PLAYER_REPUTATION) >= -100) and not(global_var(GVAR_REPUTATION_SLAVER) == 1) and not(global_var(GVAR_CHILDKILLER_REPUTATION) >= 2) and (dude_charisma > 1) and not(((party_member_count(0) - 1) >= (floor(dude_charisma / 2) + has_trait(TRAIT_PERK, dude_obj, PERK_magnetic_personality_perk))) or ((party_member_count(0) - 1) >= 5))) then begin
        giq_option(1, NAME, 3000, Node999, NEUTRAL_REACTION);
    end
    if (((party_member_count(0) - 1) >= (floor(dude_charisma / 2) + has_trait(TRAIT_PERK, dude_obj, PERK_magnetic_personality_perk))) or ((party_member_count(0) - 1) >= 5)) then begin
        giq_option(1, NAME, 3300, Node999, NEUTRAL_REACTION);
    end
    if ((dude_charisma == 1) or (global_var(GVAR_REPUTATION_SLAVER) == 1) or (global_var(GVAR_CHILDKILLER_REPUTATION) >= 2) or (global_var(GVAR_PLAYER_REPUTATION) < -100)) then begin
        giq_option(1, NAME, 3100, Node999, BAD_REACTION);
        giq_option(1, NAME, 3200, Node999, BAD_REACTION);
    end
end

