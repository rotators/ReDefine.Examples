procedure start
begin
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, 49);
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_WIMPY_GECKO);
end

procedure critter_p_proc
begin
    if ((random(0, 20) == 1) and (In_Timed_Event == 0)) then begin
        In_Timed_Event := 1;
        add_timer_event(self_obj, game_ticks(5), 1);
    end
    if ((obj_pid(self_obj) == 16777297) and obj_can_see_obj(self_obj, dude_obj)) then begin
        attack(dude_obj);
    end
    if (local_var(0) == 1) then begin
        attack(dude_obj);
    end
end

procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        In_Timed_Event := 0;
        animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), random(0, 5), random(3, 6)), 0);
    end
end

procedure destroy_p_proc
begin
    variable LVar0 := 0;
    if (obj_pid(self_obj) == 16777296) then begin
        set_map_var(1, map_var(1) - 1);
    end
    else begin
        if (obj_pid(self_obj) == 16777297) then begin
            set_map_var(2, map_var(2) - 1);
        end
    end
    if (has_trait(TRAIT_PERK, dude_obj, PERK_gecko_skinning_perk)) then begin
        LVar0 := create_object(276, 0, 0);
        add_obj_to_inven(self_obj, LVar0);
    end
end

procedure combat_p_proc
begin
    if (fixed_param == 2) then begin
        if (random(0, 100) == 1) then begin
            if (Sneer == 0) then begin
                if (source_obj == dude_obj) then begin
                    display_mstr(100);
                    Sneer := 1;
                end
                else begin
                    display_msg(mstr(101) + obj_name(source_obj));
                end
            end
            else begin
                Sneer := 0;
            end
        end
    end
end

procedure damage_p_proc
begin
    if (source_obj == dude_obj) then begin
        set_local_var(0, 1);
    end
end
