procedure critter_p_proc
begin
    if (CUR_MAP_ARDEAD) then begin
        if (local_var(4) == 0) then begin
            if (town_known(15) == MARK_STATE_UNKNOWN) then begin
                debug_msg("  mark_on_map(" + 15 + ")");
                mark_area_known(MARK_TYPE_TOWN, 15, MARK_STATE_KNOWN);
            end
            if (global_var(GVAR_FALLOUT_2) < 4) then begin
                set_global_var(GVAR_FALLOUT_2, 4);
            end
            dialogue_system_enter;
        end
    end
    else begin
        if (global_var(GVAR_ENEMY_ARROYO) == 1) then begin
            attack(dude_obj);
        end
        else begin
            if (obj_can_see_obj(self_obj, dude_obj) and (map_var(0) == 1)) then begin
                set_map_var(0, 2);
                float_msg(self_obj, mstr(218), FLOAT_MSG_YELLOW);
            end
            else begin
                if (obj_can_see_obj(self_obj, dude_obj) and (map_var(0) == 3)) then begin
                    set_map_var(0, 4);
                    float_msg(self_obj, mstr(219), FLOAT_MSG_YELLOW);
                end
                else begin
                    if (obj_can_see_obj(self_obj, dude_obj) and (map_var(0) == 5)) then begin
                        set_map_var(0, 6);
                        float_msg(self_obj, mstr(220), FLOAT_MSG_YELLOW);
                        set_local_var(5, 2);
                    end
                end
            end
        end
    end
    if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
        set_local_var(5, 1);
        attack(dude_obj);
    end
end

