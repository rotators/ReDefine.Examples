                                                start_gdialog(384, self_obj, 4, -1, -1);
                                                gsay_start;
                                                call lasher07();
                                                gsay_end;
                                                end_dialogue;
                                            end
                                        end
                                    end
                                end
                            end
                        end
                        set_local_var(0, 1);
                    end
                end
            end
        end
    end
end

