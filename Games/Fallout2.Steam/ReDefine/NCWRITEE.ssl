procedure floater_rand_with_check_cust(variable arg0, variable arg1, variable arg2)
begin
    variable LVar3 := 0;
    LVar3 := random(arg0, arg1);
    if ((LVar3 == 204) or (LVar3 == 205) or (LVar3 == 240) or (LVar3 == 241) or (LVar3 == 242) or (LVar3 == 245) or (LVar3 == 250) or (LVar3 == 255) or (LVar3 == 335) or (LVar3 == 350)) then begin
        if (LVar3 == 335) then begin
            LVar3 := "" + mstr(LVar3) + message_str(SCRIPT_NEWR1, 300 + (global_var(GVAR_NEW_RENO_MADE_MAN) % 100)) + mstr(LVar3 + 1000);
        end
        else begin
            LVar3 := "" + mstr(LVar3) + arg2 + mstr(LVar3 + 1000);
        end
        float_msg(self_obj, LVar3, FLOAT_MSG_YELLOW);
    end
    else begin
        float_msg(self_obj, mstr(LVar3), FLOAT_MSG_YELLOW);
    end
end
