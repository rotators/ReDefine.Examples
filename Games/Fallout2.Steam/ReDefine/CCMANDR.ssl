procedure critter_p_proc
begin
    if (obj_can_see_obj(self_obj, dude_obj)) then begin
        if (local_var(5) == 2) then begin
            set_local_var(5, 1);
            attack(dude_obj);
        end
        else begin
            if ((global_var(GVAR_NAVARRO_BASE_ALERT) > 0) and (local_var(11) == 0)) then begin
                dialogue_system_enter;
            end
        end
    end
end

procedure pickup_p_proc
begin
    if (source_obj == dude_obj) then begin
        set_global_var(GVAR_NAVARRO_BASE_ALERT, 1);
        set_local_var(11, 1);
        float_msg(self_obj, mstr(105), FLOAT_MSG_YELLOW);
    end
end

procedure talk_p_proc
begin
    start_gdialog(721, self_obj, 4, -1, -1);
    gsay_start;
    if (global_var(GVAR_NAVARRO_BASE_ALERT) > 0) then begin
        call Node006();
    end
    else begin
        if (map_var(5) == 1) then begin
            call Node004();
        end
        else begin
            if (map_var(6) == 1) then begin
                call Node003();
            end
            else begin
                if (local_var(4) == 0) then begin
                    set_local_var(4, 1);
                    call Node001();
                end
                else begin
                    call Node002();
                end
            end
        end
    end
    gsay_end;
    end_dialogue;
end

