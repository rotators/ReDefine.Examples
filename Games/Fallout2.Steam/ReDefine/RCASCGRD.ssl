procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        if (obj_can_see_obj(self_obj, dude_obj) and ((obj_item_subtype(critter_inven_obj(dude_obj, 1)) == 3) or (obj_item_subtype(critter_inven_obj(dude_obj, 2)) == 3))) then begin
            float_msg(self_obj, mstr(random(143, 152)), FLOAT_MSG_YELLOW);
            add_timer_event(self_obj, game_ticks(20), 2);
        end
        else begin
            add_timer_event(self_obj, game_ticks(20), 1);
        end
    end
    else begin
        if (fixed_param == 2) then begin
            if (obj_can_see_obj(self_obj, dude_obj) and ((obj_item_subtype(critter_inven_obj(dude_obj, 1)) == 3) or (obj_item_subtype(critter_inven_obj(dude_obj, 2)) == 3))) then begin
                float_msg(self_obj, mstr(random(133, 142)), FLOAT_MSG_YELLOW);
                attack(dude_obj);
            end
        end
        else begin
            if (fixed_param == 3) then begin
                float_msg(self_obj, mstr(random(163, 172)), FLOAT_MSG_YELLOW);
                add_timer_event(self_obj, game_ticks(random(20, 40)), 3);
            end
            else begin
                if (fixed_param == 4) then begin
                    if (map_var(4) == 1) then begin
                        set_map_var(4, 0);
                    end
                end
            end
        end
    end
end

