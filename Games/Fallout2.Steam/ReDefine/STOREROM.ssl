procedure start;


procedure start
begin
    if (script_action == spatial_proc) then begin
        if (source_obj == dude_obj) then begin
            if (local_var(0) != 1) then begin
                display_msg(message_str(SCRIPT_MCDAVIN, 100));
                set_local_var(0, 1);
            end
        end
    end
end
