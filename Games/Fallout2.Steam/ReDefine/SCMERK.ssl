procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        Active_Map := shouldBeHere();
        if (Active_Map) then begin
            call enterMap();
            add_timer_event(self_obj, 60 * 10, 2);
        end
        else begin
            add_timer_event(self_obj, 60 * 10, 1);
        end
    end
    else begin
        if (fixed_param == 2) then begin
            Active_Map := shouldBeHere();
            if (not(Active_Map)) then begin
                call exitMap();
                add_timer_event(self_obj, 60 * 10, 1);
            end
            else begin
                if ((tile_num(self_obj) != whatTile()) and (should_animate == 0)) then begin
                    should_animate := whatTile();
                    call ensureAnimation();
                end
                add_timer_event(self_obj, 60 * 10, 2);
            end
        end
        else begin
            if (fixed_param == 3) then begin
                if (CUR_MAP_NCRENT) then begin
                    if (tile_num(self_obj) == 29133) then begin
                        set_obj_visibility(self_obj, 1);
                        merk_visible := 0;
                    end
                    else begin
                        add_timer_event(self_obj, game_ticks(2), 3);
                    end
                end
                else begin
                    if (CUR_MAP_NCR2) then begin
                        if (tile_num(self_obj) == 14096) then begin
                            set_obj_visibility(self_obj, 1);
                            merk_visible := 0;
                        end
                        else begin
                            add_timer_event(self_obj, 60 * 10, 3);
                        end
                    end
                end
            end
            else begin
                if (fixed_param == 4) then begin
                    call ensureAnimation();
                    if (should_animate == 0) then begin
                        if (CUR_MAP_NCRENT) then begin
                            anim(self_obj, 1000, 1);
                        end
                    end
                end
                else begin
                    if (fixed_param == 5) then begin
                        if (obj_can_see_obj(self_obj, dude_obj)) then begin
                            call doMerkAttack();
                        end
                    end
                end
            end
        end
    end
end

