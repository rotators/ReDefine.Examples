procedure critter_p_proc
begin
    if (((global_var(GVAR_NEW_RENO_FLAG_1) bwand 2) != 0) and (((local_var(4) bwand 256) != 0) == 0)) then begin
        if (tile_distance_objs(self_obj, dude_obj) <= 15) then begin
            set_local_var(4, local_var(4) bwor 256);
        end
    end
    if (obj_can_see_obj(self_obj, dude_obj)) then begin
        if (((local_var(4) bwand 2) != 0) or ((global_var(251 + 3 - 1) bwand 1) != 0) or ((local_var(4) bwand 256) != 0)) then begin
            if (critter_is_fleeing(self_obj)) then begin
                if (anim_busy(self_obj) == 0) then begin
                    if ((tile_distance_objs(self_obj, dude_obj) < 8) or obj_can_see_obj(self_obj, dude_obj)) then begin
                        if (anim_busy(self_obj) == 0) then begin
                            global_temp := rotation_to_tile(dude_tile, tile_num(self_obj));
                            animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), global_temp, random(3, 10)), 1);
                            global_temp := (global_temp + 1) % 6;
                            while ((anim_busy(self_obj) == 0) and (global_temp != rotation_to_tile(dude_tile, tile_num(self_obj)))) do begin
                                debug_msg("flee loop: rot == " + global_temp);
                                animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), global_temp, random(3, 10)), 1);
                                global_temp := (global_temp + 1) % 6;
                            end
                        end
                    end
                end
            end
            else begin
                attack(dude_obj);
            end
        end
        else begin
            if (obj_is_visible_flag(self_obj) and (elevation(self_obj) == dude_elevation)) then begin
                if (new_reno_mordino_men_obj == 0) then begin
                    new_reno_mordino_men_obj := self_obj;
                end
                else begin
                    if ((obj_can_see_obj(new_reno_mordino_men_obj, dude_obj) == 0) or (critter_state(new_reno_mordino_men_obj) bwand 1)) then begin
                        new_reno_mordino_men_obj := self_obj;
                    end
                end
            end
        end
    end
    else begin
        if ((local_var(4) bwand 256) != 0) then begin
            if (tile_distance_objs(self_obj, dude_obj) > 2) then begin
                if (tile_distance_objs(self_obj, dude_obj) > (2 * 2)) then begin
                    animate_move_obj_to_tile(self_obj, tile_num_in_direction(dude_tile, (obj_get_rot(dude_obj) + 3) % 6, 2), 1);
                end
                else begin
                    animate_move_obj_to_tile(self_obj, tile_num_in_direction(dude_tile, (obj_get_rot(dude_obj) + 3) % 6, 2), 0);
                end
            end
        end
    end
end

