procedure critter_p_proc
begin
    if (obj_can_see_obj(self_obj, dude_obj) and (local_var(4) == 2)) then begin
        set_local_var(4, 1);
        attack(dude_obj);
        set_map_var(7, 1);
    end
    else begin
        if (critter_is_fleeing(self_obj)) then begin
            if ((tile_distance_objs(self_obj, dude_obj) < 8) or obj_can_see_obj(self_obj, dude_obj)) then begin
                animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), rotation_to_tile(dude_tile, tile_num(self_obj)), random(6, 10)), 1);
            end
        end
        else begin
            if (called_guards) then begin
                set_map_var(7, 1);
            end
            else begin
                if ((tile_distance_objs(self_obj, dude_obj) > 5) and (obj_get_rot(self_obj) != 2)) then begin
                    anim(self_obj, 1000, 2);
                end
            end
        end
    end
end

