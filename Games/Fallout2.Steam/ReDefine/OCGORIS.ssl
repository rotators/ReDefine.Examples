                if (obj_in_party(self_obj)) then begin
                    debug_msg("abandon party: " + self_name);
                    party_remove(self_obj);
                end
                if ((obj_pid(self_obj) == 16777380) or (obj_pid(self_obj) == 16777379)) then begin
                    debug_msg("start: set_dude_was_married");
