                        debug_msg("   GVAR_PLAYER_WAS_MARRIED after == " + global_var(GVAR_PLAYER_WAS_MARRIED));
                        set_global_var(GVAR_PLAYER_MARRIED, 0);
                    end
                    debug_msg("finished: set_dude_was_married");
                end
                if ((local_var(11) != -1) and (has_trait(TRAIT_OBJECT, self_obj, OBJECT_TEAM_NUM) == 0)) then begin
                    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, local_var(11));
