            party_remove(self_obj);
            set_obj_visibility(self_obj, 0);
        end
    end
    else begin
        if (local_var(5) and obj_can_see_obj(self_obj, dude_obj)) then begin
            if (critter_is_fleeing(self_obj)) then begin
