procedure start
begin
    if (script_action == talk_proc) then begin
        call do_dialogue();
    end
    else begin
        if ((script_action == look_at_proc) or (script_action == description_proc)) then begin
            script_overrides;
            display_msg(message_str(0, 100));
        end
        else begin
            if (script_action == critter_proc) then begin
                if (hostile) then begin
                    hostile := 0;
                    attack(dude_obj);
                end
            end
        end
    end
end

procedure do_dialogue
begin
    call weapon_check();
    start_dialogue(self_obj, 4);
    sayStart;
    if (global_var(GVAR_NCR_VORTIS_QUEST_STATE) == 1) then begin
        call surf00();
    end
    else begin
        if (global_var(GVAR_GECKO_ECON_DISK) > 2) then begin
            call surf00();
        end
        else begin
            if ((global_var(GVAR_GECKO_REQ_FORM) > (2 * global_var(GVAR_GECKO_SKEETER_PART))) or (global_var(GVAR_SIERRA_LOCKOUT) == 1)) then begin
                call surf00();
            end
            else begin
                if (local_var(6) == 1) then begin
                    call surf02();
                end
                else begin
                    if ((dude_charisma > 6) and (GENDER == 1)) then begin
                        call surf01();
                    end
                    else begin
                        if (local_var(5) == 1) then begin
                            call surf03();
                        end
                        else begin
                            if (armed == 1) then begin
                                call surf04();
                            end
                            else begin
                                if (obj_pid(critter_inven_obj(dude_obj, INVEN_TYPE_WORN)) == 113) then begin
                                    call surf05();
                                end
                                else begin
                                    if (POWERBLOWN == 1) then begin
                                        call surf06();
                                    end
                                    else begin
                                        call surf07();
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    sayEnd;
    end_dialogue;
end

