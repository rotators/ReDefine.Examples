procedure timed_event_p_proc
begin
    variable LVar0 := 0;
    if (fixed_param == 1) then begin
        LVar0 := getTimeBit();
        if ((LVar0 bwand 128) and (called8 == 0)) then begin
            called8 := 1;
            called24 := 0;
            call time_8_hour();
        end
        if ((LVar0 bwand 8388608) and (called24 == 0)) then begin
            called8 := 0;
            called24 := 1;
            call time_24_hour();
        end
        if (called8) then begin
            if (not(anim_busy(self_obj))) then begin
                if (random(1, 10) == 1) then begin
                    reg_anim_func(2, self_obj);
                    reg_anim_func(1, 1);
