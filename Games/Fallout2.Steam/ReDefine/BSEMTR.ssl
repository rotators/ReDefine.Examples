            display_mstr(104);
            add_timer_event(self_obj, 0, 3);
            add_timer_event(self_obj, game_ticks(1), 2);
        end
        else begin
            if (i_field_obj != -1) then begin
                add_timer_event(i_field_obj, 0, 4);
