procedure critter_p_proc
begin
    call rustle_status();
    if ((local_var(5) == 0) and (map_var(18) > 0)) then begin
        call Node021();
        set_local_var(5, 2);
    end
    if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
        set_local_var(5, 1);
        attack(dude_obj);
    end
    else begin
        if (local_var(10) == 1) then begin
            dtile := dude_tile;
            if (tile_distance(dtile, 19450) < 14) then begin
                call leave_player();
            end
            else begin
                mytile := tile_num(self_obj);
                if (tile_distance(mytile, dtile) > 5) then begin
                    newtile := tile_num_in_direction(dtile, random(0, 5), 1);
                    animate_move_obj_to_tile(self_obj, newtile, 1);
                end
            end
        end
        else begin
            if (local_var(10) == 0) then begin
                if (CUR_MAP_KLADWTWN) then begin
                    if (random(1, 100) and (In_Timed_Event == 0)) then begin
                        debug_msg("Torr can move");
