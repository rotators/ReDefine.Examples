procedure critter_p_proc
begin
    variable LVar0 := 0;
    if (obj_can_see_obj(self_obj, dude_obj)) then begin
        attack_complex(dude_obj, 0, 1, 0, 0, 30000, 0, 0);
    end
    if (not(anim_busy(self_obj))) then begin
        reg_anim_func(2, self_obj);
        reg_anim_func(1, 1);
        if (obj_pid(self_obj) != 16777570) then begin
            animate_move_obj_to_tile(self_obj, tile_num_in_direction(starttile, random(0, 5), random(3, 10)), 0);
        end
        else begin
            LVar0 := tile_num_in_direction(starttile, random(0, 5), random(3, 8));
            if (tile_in_tile_rect(16728, 16718, 24728, 24718, LVar0)) then begin
                animate_move_obj_to_tile(self_obj, LVar0, 0);
            end
        end
        reg_anim_func(3, 0);
    end
end

procedure destroy_p_proc
begin
    set_global_var(461, global_var(461) - 1);
    if ((global_var(461) == 0) and (global_var(389) != 2)) then begin
        set_global_var(389, 2);
        display_msg(message_str(14, 1014));
        give_exp_points(3500);
        display_msg(message_str(14, 100) + 3500 + message_str(14, 101));
    end
end

