procedure map_enter_p_proc
begin
    critter_add_trait(self_obj, 1, 6, 197);
    critter_add_trait(self_obj, 1, 5, 10);
    starttile := tile_num(self_obj);
    reg_anim_func(2, self_obj);
    reg_anim_func(1, 1);
    anim(self_obj, 1000, random(0, 5));
    reg_anim_func(3, 0);
    if ((obj_pid(self_obj) == 16777570) and (local_var(0) == 0)) then begin
        set_local_var(0, tile_num(self_obj));
    end
end

