procedure PayDude
begin
    if (item_caps_total(dude_obj) >= Heal_Cost) then begin
        item_caps_adjust(dude_obj, -Heal_Cost);
        critter_heal(dude_obj, get_critter_stat(dude_obj, 7) - get_critter_stat(dude_obj, 35));
        critter_injure(dude_obj, 4 bwor 8 bwor 16 bwor 32 bwor 8388608);
        gfade_out(600);
        if (Heal_Cost == 300) then begin
            game_time_advance(game_ticks(8 * (60 * (60 * 10))));
        end
        else begin
            game_time_advance(game_ticks(2 * (60 * (60 * 10))));
        end
        gfade_in(600);
        gsay_reply(590, 315);
        giq_option(4, 590, 202, Node001, 50);
        giq_option(4, 590, 203, Node999, 50);
    end
    else begin
        call Node017();
    end
end

