procedure timed_event_p_proc
begin
    if (fixed_param == 0) then begin
        if (combat_is_initialized == 0) then begin
            if (tile_distance(tile_num(self_obj), local_var(7)) > 3) then begin
                reg_anim_func(2, self_obj);
                reg_anim_func(1, 1);
                animate_move_obj_to_tile(self_obj, local_var(7), 0);
                reg_anim_func(3, 0);
            end
            else begin
                reg_anim_func(2, self_obj);
                reg_anim_func(1, 1);
                animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), random(0, 5), random(1, 5)), 0);
                reg_anim_func(3, 0);
            end
            add_timer_event(self_obj, game_ticks(random(4, 10)), 0);
        end
    end
end

procedure map_exit_p_proc
begin
    rm_timer_event(self_obj);
end

procedure critter_p_proc
begin
    if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
        set_local_var(5, 2);
        attack(dude_obj);
    end
    if (elevation(self_obj) == 1) then begin
        if (obj_can_see_obj(self_obj, dude_obj)) then begin
            if (obj_is_carrying_obj_pid(dude_obj, 205) > 0) then begin
                set_local_var(5, 1);
                attack(dude_obj);
            end
            else begin
                if (tile_distance_objs(self_obj, dude_obj) < 6) then begin
                    set_local_var(5, 1);
                    attack(dude_obj);
                end
            end
        end
    end
    else begin
        if (tile_distance_objs(self_obj, dude_obj) < 8) then begin
            set_local_var(5, 1);
            attack(dude_obj);
        end
    end
end

