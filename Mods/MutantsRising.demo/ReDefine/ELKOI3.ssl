procedure timed_event_p_proc
begin
    if (fixed_param == 0) then begin
        if (rootdown == 0) then begin
            rootdown := rootdown + 1;
            display_msg(mstr(106));
            display_msg(mstr(107));
            display_msg(mstr(108));
            display_msg(mstr(109));
            add_timer_event(self_obj, game_ticks(5), 0);
        end
        else begin
            if (rootdown == 1) then begin
                rootdown := rootdown + 1;
                display_msg(mstr(110));
                display_msg(mstr(111));
                display_msg(mstr(112));
                display_msg(mstr(113));
                add_timer_event(self_obj, game_ticks(5), 0);
            end
            else begin
                if (rootdown == 2) then begin
                    rootdown := rootdown + 1;
                    display_msg(mstr(114));
                    display_msg(mstr(115));
                    display_msg(mstr(116));
                    display_msg(mstr(117));
                    add_timer_event(self_obj, game_ticks(5), 0);
                end
                else begin
                    if (rootdown == 3) then begin
                        rootdown := rootdown + 1;
                        display_msg(mstr(118));
                        display_msg(mstr(119));
                        display_msg(mstr(120));
                        display_msg(mstr(121));
                        add_timer_event(self_obj, game_ticks(5), 0);
                    end
                    else begin
                        if (rootdown == 4) then begin
                            rootdown := 0;
