procedure timed_event_p_proc
begin
    if (fixed_param == 0) then begin
        anim(self_obj, 1010, 1);
        steam := tile_contains_pid_obj(tile_num(self_obj), 0, 33557139);
        destroy_object(steam);
    end
end

procedure use_p_proc
begin
    script_overrides;
    if (map_var(18) < 1) then begin
        if (tile_num(janey_ptr) == map_var(17)) then begin
            if (using_skill(dude_obj, 8) and is_success(roll_vs_skill(dude_obj, 10, 10))) then begin
                call steal_coffee();
            end
            else begin
                reg_anim_func(2, dude_obj);
                add_timer_event(janey_ptr, 3, 0);
            end
        end
        else begin
            call steal_coffee();
        end
    end
    else begin
        display_mstr(100);
    end
end

procedure steal_coffee
begin
    if (map_var(18) < 1) then begin
        set_map_var(18, 1);
        set_map_var(19, game_time + (10 * (60 * 10)));
    end
    display_mstr(101);
    give_exp_points(10);
    display_msg(g_mstr(100) + 10 + g_mstr(101));
    add_timer_event(self_obj, 1, 0);
    item := create_object(533, 0, 0);
    add_obj_to_inven(dude_obj, item);
end
