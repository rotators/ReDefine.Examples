            create_object_sid(439, 23743, 0, -1);
        end
        if (random(0, 100) <= 50) then begin
            create_object_sid(439, 27942, 0, -1);
        end
        else begin
            create_object_sid(439, 27732, 0, -1);
        end
        if (random(0, 100) <= 50) then begin
            create_object_sid(439, 24932, 0, -1);
