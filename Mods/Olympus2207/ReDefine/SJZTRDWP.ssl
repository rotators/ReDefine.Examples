procedure critter_p_proc
begin
    if ((animation_is_called == 0) and (anim_busy(self_obj) == 0)) then begin
        if (tile_num(self_obj) == local_var(3)) then begin
            add_timer_event(self_obj, random(500, 900), 1);
            anim(self_obj, 1000, local_var(7));
        end
        else begin
            if ((tile_num(self_obj) == 13665) and (went_to_wc == 1)) then begin
                add_timer_event(self_obj, random(1, 20), 2);
            end
            else begin
                add_timer_event(self_obj, random(10, 30), 3);
            end
        end
        animation_is_called := 1;
    end
    if (obj_can_see_obj(self_obj, dude_obj)) then begin
        if ((local_var(2) == 1) or (global_var(GVAR_TOWN_REP_MODOC) <= -31)) then begin
            set_local_var(2, 0);
            attack_setup(self_obj, dude_obj);
        end
    end
end

