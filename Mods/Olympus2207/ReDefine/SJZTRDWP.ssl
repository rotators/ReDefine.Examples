procedure timed_event_p_proc
begin
    if (not(combat_is_initialized)) then begin
        if (fixed_param == 1) then begin
            if (went_to_wc == 0) then begin
                went_to_wc := 1;
                animate_move_obj_to_tile(self_obj, 13665, 0);
            end
            animation_is_called := 0;
        end
        if (fixed_param == 2) then begin
            anim(self_obj, 1000, 3);
            reg_anim_func(2, self_obj);
            reg_anim_func(1, 1);
            reg_anim_animate(self_obj, 11, -1);
            reg_anim_func(3, 0);
            animation_is_called := 0;
            went_to_wc := 0;
        end
        if (fixed_param == 3) then begin
            animate_move_obj_to_tile(self_obj, local_var(3), 0 bwor 16);
            animation_is_called := 0;
        end
    end
end

procedure Node001
begin
    Reply(random(10, 12));
    giq_option(1, NAME, 13, NodeBarter, NEUTRAL_REACTION);
    if (local_var(8) == 0) then begin
        giq_option(1, NAME, 15, Node002, NEUTRAL_REACTION);
    end
    if ((local_var(9) == 0) and (get_pc_stat(PCSTAT_level) <= 4)) then begin
        giq_option(1, NAME, 19, Node003, NEUTRAL_REACTION);
    end
    giq_option(1, NAME, 14, Node999, NEUTRAL_REACTION);
end

procedure Node002
begin
    set_local_var(8, 1);
    Reply(16);
    giq_option(1, NAME, 17, NodeBarter, NEUTRAL_REACTION);
    giq_option(1, NAME, 18, Node999, NEUTRAL_REACTION);
end

procedure Node003
begin
    set_local_var(9, 1);
    Reply(20);
    giq_option(1, NAME, 17, NodeBarter, NEUTRAL_REACTION);
