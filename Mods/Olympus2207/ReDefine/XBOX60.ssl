            display_msg(message_str(SCRIPT_STTEXT, random(140, 143)) + (floor(exp_points / 5) + (dude_iq * floor(exp_points / 12)) + (dude_luck * floor(exp_points / 30)) + (floor(exp_points / 75) * (((dude_luck - 4) * 4) - (difficulty_level * 4)))) + message_str(SCRIPT_STTEXT, 144));
