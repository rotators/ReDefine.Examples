procedure copy_items
begin
    while (item_pid_number < 560) do begin
        item_pid_number := item_pid_number + 1;
        if (obj_is_carrying_obj_pid(self_obj, item_pid_number)) then begin
            if (obj_is_carrying_obj_pid(self_obj, item_pid_number) > 1) then begin
                count := 0;
                while (obj_is_carrying_obj_pid(self_obj, item_pid_number) > count) do begin
                    restock_obj := create_object_sid(item_pid_number, 0, 0, -1);
                    add_mult_objs_to_inven(items_carrier, restock_obj, 1);
                    count := count + 1;
                end
                count := 0;
            end
            else begin
                restock_obj := create_object_sid(item_pid_number, 0, 0, -1);
                add_mult_objs_to_inven(items_carrier, restock_obj, 1);
            end
        end
    end
    count := 0;
    item_pid_number := 0;
    set_global_var(843, global_var(843) bwor 1048576);
    party_add(items_carrier);
end

procedure combat_p_proc
begin
    probability := random(0, 15);
    if ((probability < 4) and (get_critter_stat(dude_obj, 35) < 7) and not(critter_is_fleeing(self_obj))) then begin
        if ((local_var(4) == 1) and obj_can_see_obj(self_obj, dude_obj)) then begin
            float_msg(self_obj, message_str(1551, 401), 2);
        end
    end
    if ((probability >= 14) and not(critter_is_fleeing(self_obj))) then begin
        if ((probability == 14) and (local_var(4) == 1) and obj_can_see_obj(self_obj, dude_obj)) then begin
            float_msg(self_obj, message_str(1551, 401), 2);
        end
        if ((probability == 15) and (local_var(4) == 1) and obj_can_see_obj(self_obj, dude_obj) and (get_critter_stat(dude_obj, 34) == 0)) then begin
            float_msg(self_obj, message_str(1551, 403), 2);
        end
        if ((probability == 15) and (local_var(4) == 1) and obj_can_see_obj(self_obj, dude_obj) and (get_critter_stat(dude_obj, 34) == 1)) then begin
            float_msg(self_obj, message_str(1551, 404), 2);
        end
    end
end

