        set_global_var(GVAR_S_RAIDERS_KILLED, 1);
        if (global_var(GVAR_QUEST_S_FREDERYK_RAIDERS) == 1) then begin
            set_global_var(GVAR_QUEST_S_FREDERYK_RAIDERS, 2);
