            call Node5200();
        end
        else begin
            call Node4850();
        end
        gsay_end;
        end_dialogue;
    end
    if (fixed_param == 3) then begin
        add_timer_event(Sedit_LostTown, 0, 2);
        rm_fixed_timer_event(self_obj, 3);
        add_timer_event(self_obj, game_ticks(1), 3);
    end
