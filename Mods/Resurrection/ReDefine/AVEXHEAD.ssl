        if (obj_is_visible_flag(self_obj) == 0) then begin
            set_obj_visibility(self_obj, 0);
        end
    end
    else begin
        if (obj_is_visible_flag(self_obj) == 1) then begin
            set_obj_visibility(self_obj, 1);
        end
    end
end

