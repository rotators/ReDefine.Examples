procedure Wear_leather_armor
begin
    fade_out;
    game_ui_disable;
    if (not(obj_carrying_pid_obj(self_obj, 1))) then begin
        if (dude_gender == 1) then begin
            lFidArmor := 16777221;
        end
        else begin
            lFidArmor := 16777228;
        end
        lPartyWornArmor := 0;
        if (obj_pid(critter_inven_obj(dude_obj, INVEN_TYPE_WORN)) == 1) then begin
            lPartyWornArmor := 1;
            if (obj_item_subtype(critter_inven_obj(dude_obj, INVEN_TYPE_WORN)) == 0) then begin
                restock_obj := critter_inven_obj(dude_obj, INVEN_TYPE_WORN);
                debug_msg("armour pid == " + obj_pid(restock_obj));
                rm_obj_from_inven(dude_obj, restock_obj);
                add_obj_to_inven(dude_obj, restock_obj);
            end
        end
        removed_qty := obj_is_carrying_obj_pid(dude_obj, 1);
        if (1 < removed_qty) then begin
            removed_qty := 1;
        end
        if (removed_qty > 0) then begin
            restock_obj := obj_carrying_pid_obj(dude_obj, 1);
            if (obj_type(self_obj) == 1) then begin
                restock_obj := obj_carrying_pid_obj(dude_obj, 1);
                if (critter_inven_obj(self_obj, INVEN_TYPE_WORN) == restock_obj) then begin
                    if (obj_item_subtype(critter_inven_obj(dude_obj, INVEN_TYPE_WORN)) == 0) then begin
                        restock_obj := critter_inven_obj(dude_obj, INVEN_TYPE_WORN);
                        debug_msg("armour pid == " + obj_pid(restock_obj));
                        rm_obj_from_inven(dude_obj, restock_obj);
                        add_obj_to_inven(dude_obj, restock_obj);
                    end
                end
                else begin
                    if ((critter_inven_obj(dude_obj, INVEN_TYPE_LEFT_HAND) == restock_obj) or (critter_inven_obj(dude_obj, INVEN_TYPE_RIGHT_HAND) == restock_obj)) then begin
                        inven_unwield(dude_obj);
                    end
                end
            end
            restock_amt := rm_mult_objs_from_inven(dude_obj, restock_obj, removed_qty);
            if (restock_amt == 0) then begin
                toRemove_qty := 1;
                while ((toRemove_qty > 0) and obj_carrying_pid_obj(dude_obj, 1)) do begin
                    restock_obj := obj_carrying_pid_obj(dude_obj, 1);
                    rm_obj_from_inven(dude_obj, restock_obj);
                    destroy_object(restock_obj);
                    toRemove_qty := toRemove_qty - 1;
                end
            end
            else begin
                destroy_object(restock_obj);
            end
        end
        restock_obj := 0;
        restock_amt := 0;
        restock_obj := create_object(1, 0, 0);
        add_mult_objs_to_inven(self_obj, restock_obj, 1);
        if (lPartyWornArmor == 1) then begin
            wield_obj_critter(dude_obj, obj_carrying_pid_obj(dude_obj, 1));
            art_change_fid_num(dude_obj, lFidArmor);
        end
    end
    wield_obj(obj_carrying_pid_obj(self_obj, 1));
    add_timer_event(self_obj, 6, 1);
end

