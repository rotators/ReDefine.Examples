procedure Set_time_to_arrival_date
begin
    if (CUR_MAP_ALB_PRED) then begin
        if ((get_day == 12) or (get_day == 27)) then begin
            if (game_time_hour > 1200) then begin
                exit_checking_loop := 1;
            end
            else begin
                game_time_advance(1 * (60 * (60 * 10)));
            end
        end
        else begin
            game_time_advance(1 * (24 * (60 * (60 * 10))));
        end
    end
    if (CUR_MAP_CORATH1) then begin
        if ((get_day == 9) or (get_day == 23)) then begin
            if (game_time_hour > 1200) then begin
                exit_checking_loop := 1;
            end
            else begin
                game_time_advance(1 * (60 * (60 * 10)));
            end
        end
        else begin
            game_time_advance(1 * (24 * (60 * (60 * 10))));
        end
    end
    if (CUR_MAP_NH_VILL) then begin
        if ((get_day == 1) or (get_day == 17)) then begin
            if (game_time_hour > 1200) then begin
                exit_checking_loop := 1;
            end
            else begin
                game_time_advance(1 * (60 * (60 * 10)));
            end
        end
        else begin
            game_time_advance(1 * (24 * (60 * (60 * 10))));
        end
    end
    checking_loop_pojistka := checking_loop_pojistka + 1;
    if (checking_loop_pojistka == 45) then begin
        exit_checking_loop := 1;
    end
    if (exit_checking_loop == 0) then begin
        call Set_time_to_arrival_date();
    end
end

