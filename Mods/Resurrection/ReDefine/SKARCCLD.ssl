    if ((CUR_MAP_ALB_PRED) and ((global_var(GVAR_KARAVAN_ROUTES_COMMAND_BITS) bwand 1) != 0) and ((global_var(GVAR_KARAVAN_ROUTES_COMMAND_BITS) bwand 16) != 0) and not((global_var(GVAR_KARAVAN_ROUTES_COMMAND_BITS) bwand 512) != 0)) then begin
