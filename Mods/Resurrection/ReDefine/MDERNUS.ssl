procedure Node1900
begin
    locker := tile_contains_pid_obj(16498, 1, 188);
    if (obj_is_carrying_obj_pid(locker, 3) > 0) then begin
        Reply(1900);
        removed_qty := obj_is_carrying_obj_pid(locker, 3);
        if (1 < removed_qty) then begin
            removed_qty := 1;
        end
        if (removed_qty > 0) then begin
            restock_obj := obj_carrying_pid_obj(locker, 3);
            if (obj_type(self_obj) == 1) then begin
                restock_obj := obj_carrying_pid_obj(locker, 3);
                if (critter_inven_obj(self_obj, INVEN_TYPE_WORN) == restock_obj) then begin
                    if (obj_item_subtype(critter_inven_obj(locker, INVEN_TYPE_WORN)) == 0) then begin
                        restock_obj := critter_inven_obj(locker, INVEN_TYPE_WORN);
                        debug_msg("armour pid == " + obj_pid(restock_obj));
                        rm_obj_from_inven(locker, restock_obj);
                        add_obj_to_inven(locker, restock_obj);
                    end
                end
                else begin
                    if ((critter_inven_obj(locker, INVEN_TYPE_LEFT_HAND) == restock_obj) or (critter_inven_obj(locker, INVEN_TYPE_RIGHT_HAND) == restock_obj)) then begin
                        inven_unwield(locker);
                    end
                end
            end
            restock_amt := rm_mult_objs_from_inven(locker, restock_obj, removed_qty);
            if (restock_amt == 0) then begin
                toRemove_qty := 1;
                while ((toRemove_qty > 0) and obj_carrying_pid_obj(locker, 3)) do begin
                    restock_obj := obj_carrying_pid_obj(locker, 3);
                    rm_obj_from_inven(locker, restock_obj);
                    destroy_object(restock_obj);
                    toRemove_qty := toRemove_qty - 1;
                end
            end
            else begin
                destroy_object(restock_obj);
            end
        end
        restock_obj := 0;
        restock_amt := 0;
        restock_obj := create_object(3, 0, 0);
        add_mult_objs_to_inven(dude_obj, restock_obj, 1);
    end
    else begin
        Reply(1905);
    end
    set_global_var(GVAR_MC_QUEST_BRING_SEBASTIAN, 4);
    give_exp_points(5000);
