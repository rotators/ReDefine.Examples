procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        sequence := sequence + 1;
        if (tile_distance_objs(self_obj, dude_obj) <= 2) then begin
            if (sequence == 6) then begin
                rm_fixed_timer_event(self_obj, 2);
                add_timer_event(self_obj, game_ticks(0), 2);
            end
            else begin
                rm_fixed_timer_event(self_obj, 1);
                add_timer_event(self_obj, game_ticks(1), 1);
            end
        end
        else begin
            sequence := 0;
        end
    end
    if (fixed_param == 2) then begin
        float_msg(self_obj, mstr(301), FLOAT_MSG_YELLOW);
        add_timer_event(self_obj, game_ticks(10), 3);
    end
    if (fixed_param == 3) then begin
        sequence := 0;
    end
    if (fixed_param == 10) then begin
        set_local_var(5, 2);
    end
end

