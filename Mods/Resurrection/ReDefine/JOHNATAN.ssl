        gsay_start;
        call Node1650();
        gsay_end;
        end_dialogue;
        projed_smycku := 1;
    end
    if ((local_var(5) == 1) and (projed_smycku == 0) and ((global_var(GVAR_QUEST_JOHNATAN_CAVE) == 1) or (global_var(GVAR_QUEST_JOHNATAN_CAVE) == 2))) then begin
