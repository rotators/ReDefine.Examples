                toRemove_qty := toRemove_qty - 1;
            end
        end
        else begin
            destroy_object(restock_obj);
        end
    end
    restock_obj := 0;
    restock_amt := 0;
    restock_obj := create_object(98, 0, 0);
    add_mult_objs_to_inven(self_obj, restock_obj, 1);
    set_global_var(GVAR_QUEST_S_CHARLIE_JUNK, 2);
    item_caps_adjust(dude_obj, 400);
    give_exp_points(500);
    display_msg(g_mstr(100) + 500 + g_mstr(101));
    set_local_var(5, 1);
    giq_option(-3, NAME, 2252, Node2300, NEUTRAL_REACTION);
end

