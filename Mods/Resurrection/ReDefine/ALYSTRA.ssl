procedure JOIN_TO_PARTY
begin
    lystra_partym := create_object_sid(16777814, tile_num(self_obj), elevation(self_obj), SCRIPT_LYSTRA);
    anim(lystra_partym, 1000, obj_get_rot(self_obj));
    party_add(lystra_partym);
    critter_add_trait(lystra_partym, TRAIT_OBJECT, OBJECT_TEAM_NUM, 0);
    move_obj_inven_to_obj(self_obj, lystra_partym);
    restock_obj := create_object(24, 0, 0);
    add_mult_objs_to_inven(lystra_partym, restock_obj, 1);
    restock_obj := create_object(38, 0, 0);
    add_mult_objs_to_inven(lystra_partym, restock_obj, 1);
    restock_obj := create_object(579, 0, 0);
    add_mult_objs_to_inven(lystra_partym, restock_obj, 1);
    wield_obj_critter(lystra_partym, obj_carrying_pid_obj(lystra_partym, 579));
    art_change_fid_num(lystra_partym, 16777223);
    if (not((global_var(GVAR_PTR_PARTY_MEMBER_ACTIONS) bwand 16) != 0)) then begin
        set_global_var(GVAR_PTR_PARTY_MEMBER_ACTIONS, global_var(GVAR_PTR_PARTY_MEMBER_ACTIONS) bwor 16);
        give_exp_points(100);
        display_msg(g_mstr(100) + 100 + g_mstr(101));
    end
    set_global_var(GVAR_PTR_LYSTRA_REACTIONS_1, global_var(GVAR_PTR_LYSTRA_REACTIONS_1) bwor 4);
    destroy_after_join := 1;
    add_timer_event(lystra_partym, 0, 39);
    add_timer_event(self_obj, 2, 2);
    call Node999();
end
