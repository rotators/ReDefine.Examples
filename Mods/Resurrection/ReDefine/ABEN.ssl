procedure vypocet
begin
    aleeyadays := (game_time - local_var(7)) / (24 * (60 * (60 * 10)));
    while (aleeyadays > 0) do begin
        aleeyarest := aleeyarest + 1;
        if (aleeyarest >= 7) then begin
            aleeyacaps := aleeyacaps + 1;
            aleeyarest := 0;
        end
        aleeyadays := aleeyadays - 1;
    end
    while (aleeyacaps > 0) do begin
        if (dude_luck >= 6) then begin
            if (dude_luck == 10) then begin
                probability := random(6, 10);
            end
            if (dude_luck == 9) then begin
                probability := random(5, 10);
            end
            if (dude_luck == 8) then begin
                probability := random(4, 10);
            end
            if (dude_luck == 7) then begin
                probability := random(3, 10);
            end
            if (dude_luck == 6) then begin
                probability := random(2, 10);
            end
        end
        else begin
            if (dude_luck == 5) then begin
                probability := random(0, 8);
            end
            if (dude_luck == 4) then begin
                probability := random(0, 7);
            end
            if (dude_luck == 3) then begin
                probability := random(0, 6);
            end
            if (dude_luck == 2) then begin
                probability := random(0, 5);
            end
            if (dude_luck <= 1) then begin
                probability := random(0, 4);
            end
        end
        if (probability == 10) then begin
            weekly_pay := weekly_pay + 200;
        end
        if (probability == 9) then begin
            weekly_pay := weekly_pay + 190;
        end
        if (probability == 8) then begin
            weekly_pay := weekly_pay + 180;
        end
        if (probability == 7) then begin
            weekly_pay := weekly_pay + 170;
        end
        if (probability == 6) then begin
            weekly_pay := weekly_pay + 160;
        end
        if (probability == 5) then begin
            weekly_pay := weekly_pay + 150;
        end
        if (probability == 4) then begin
            weekly_pay := weekly_pay + 140;
        end
        if (probability == 3) then begin
            weekly_pay := weekly_pay + 130;
        end
        if (probability == 2) then begin
            weekly_pay := weekly_pay + 120;
        end
        if (probability == 1) then begin
            weekly_pay := weekly_pay + 110;
        end
        if (probability == 0) then begin
            weekly_pay := weekly_pay + 100;
        end
        aleeyacaps := aleeyacaps - 1;
    end
    set_local_var(7, game_time - (24 * (60 * (60 * 10)) * aleeyarest));
end
