procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        game_ui_enable;
        fade_in;
        animate_move_obj_to_tile(self_obj, 14485, 0);
        set_local_var(9, 1);
        rm_fixed_timer_event(self_obj, 2);
        add_timer_event(self_obj, game_ticks(1), 2);
    end
    if (fixed_param == 2) then begin
        if (tile_num(self_obj) == 14485) then begin
            set_local_var(9, 0);
            anim(self_obj, 1000, 2);
        end
        else begin
            rm_fixed_timer_event(self_obj, 2);
            add_timer_event(self_obj, game_ticks(1), 2);
        end
    end
end

procedure map_enter_p_proc
begin
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, 212);
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_COWARD_NO_TALKING);
    if (tile_num(self_obj) != 14485) then begin
        animate_move_obj_to_tile(self_obj, 14485, 0);
        set_local_var(9, 1);
        rm_fixed_timer_event(self_obj, 2);
        add_timer_event(self_obj, game_ticks(1), 2);
    end
    if (not(is_loading_game)) then begin
        if (global_var(GVAR_DUDE_NICOL_MEETING) == 1) then begin
            set_obj_visibility(self_obj, 1);
        end
        else begin
            set_obj_visibility(self_obj, 0);
        end
    end
end

