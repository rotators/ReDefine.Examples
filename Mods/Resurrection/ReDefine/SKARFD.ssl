procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        if (not(anim_busy(self_obj))) then begin
            reg_anim_func(2, self_obj);
            reg_anim_func(1, 1);
            animate_move_obj_to_tile(self_obj, tile_num_in_direction(local_var(6), random(0, 5), random(1, 3)), 0);
            reg_anim_func(3, 0);
        end
        rm_fixed_timer_event(self_obj, 1);
        add_timer_event(self_obj, game_ticks(random(30, 60)), 1);
    end
end

procedure map_enter_p_proc
begin
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, 212);
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_CARAVAN_GUARD);
    if (not(is_loading_game)) then begin
        if (local_var(6) == 0) then begin
            set_local_var(6, tile_num(self_obj));
        end
        reg_anim_func(2, self_obj);
        reg_anim_func(1, 1);
        anim(self_obj, 1000, random(0, 5));
        reg_anim_func(3, 0);
        rm_fixed_timer_event(self_obj, 1);
        add_timer_event(self_obj, game_ticks(random(30, 60)), 1);
    end
    if ((game_time_hour <= 600) or (game_time_hour >= 1800) and (global_var(GVAR_QUEST_S_NICOL_BRAHMINS) == 1)) then begin
        set_obj_visibility(self_obj, 1);
    end
    else begin
        set_obj_visibility(self_obj, 0);
    end
end

