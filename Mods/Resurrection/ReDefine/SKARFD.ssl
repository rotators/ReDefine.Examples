procedure talk_p_proc
begin
    while (dialog_loop == 0) do begin
        talking_float := random(0, 19);
        loop_numbers := loop_numbers + 1;
        if ((talking_float >= 0) and (talking_float <= 9) or (loop_numbers == 7)) then begin
            float_msg(self_obj, mstr(random(300, 309)), FLOAT_MSG_YELLOW);
            dialog_loop := 1;
        end
        if ((talking_float == 10) and (global_var(GVAR_FROST_DESTROY) != 1)) then begin
            float_msg(self_obj, mstr(310), FLOAT_MSG_YELLOW);
            dialog_loop := 1;
        end
        if ((talking_float == 11) and (global_var(GVAR_QUEST_S_KEVIN_MEETING) == 3)) then begin
            float_msg(self_obj, mstr(311), FLOAT_MSG_YELLOW);
            dialog_loop := 1;
        end
        if ((talking_float >= 12) and (talking_float <= 13) and (map_var(2) != 1)) then begin
            if (global_var(GVAR_QUEST_S_KEVIN_MEETING) == 3) then begin
                float_msg(self_obj, mstr(random(314, 315)), FLOAT_MSG_YELLOW);
            end
            else begin
                float_msg(self_obj, mstr(random(312, 313)), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
        if ((talking_float >= 14) and (talking_float <= 16) and (map_var(2) == 1)) then begin
            if (dude_gender == 0) then begin
                float_msg(self_obj, mstr(random(316, 318)), FLOAT_MSG_YELLOW);
            end
            if (dude_gender == 1) then begin
                float_msg(self_obj, mstr(random(317, 319)), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
        if ((talking_float == 17) and (global_var(GVAR_FREDERYKS_DREAM_LEAVE) == 1)) then begin
            if (global_var(GVAR_QUEST_S_KEVIN_MEETING) == 3) then begin
                float_msg(self_obj, mstr(320), FLOAT_MSG_YELLOW);
            end
            else begin
                float_msg(self_obj, mstr(321), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
        if ((talking_float >= 18) and (talking_float <= 19) and (global_var(GVAR_FREDERYKS_DREAM_LEAVE) == 1)) then begin
            if (dude_gender == 0) then begin
                float_msg(self_obj, mstr(random(322, 323)), FLOAT_MSG_YELLOW);
            end
            if (dude_gender == 1) then begin
                float_msg(self_obj, mstr(random(323, 324)), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
    end
    dialog_loop := 0;
    loop_numbers := 0;
end

