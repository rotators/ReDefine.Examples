        float_msg(self_obj, mstr(304), FLOAT_MSG_RED);
    end
    else begin
        if (global_var(GVAR_AQ_MUTANT_HUNTER) == 1) then begin
            if ((global_var(GVAR_AQ_ALBUQ_DEATHS) bwand 1) != 0) then begin
                GVar21 := random(0, 11);
                if (GVar21 > 3) then begin
                    float_msg(self_obj, mstr(random(321, 327)), FLOAT_MSG_YELLOW);
                end
                else begin
                    float_msg(self_obj, mstr(random(307, 319)), FLOAT_MSG_YELLOW);
                end
            end
            else begin
                if (dude_gender == 0) then begin
                    float_msg(self_obj, mstr(random(306, 319)), FLOAT_MSG_YELLOW);
                end
                else begin
                    float_msg(self_obj, mstr(random(307, 320)), FLOAT_MSG_YELLOW);
                end
            end
        end
        else begin
            anim(dude_obj, 1000, rotation_to_tile(dude_tile, tile_num(self_obj)));
            anim(self_obj, 1000, rotation_to_tile(tile_num(self_obj), dude_tile));
            start_gdialog(1699, self_obj, 4, -1, -1);
            gsay_start;
            call Node500();
            gsay_end;
            end_dialogue;
        end
    end
end

