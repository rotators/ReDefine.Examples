procedure Node_Give_Bag
begin
    set_local_var(10, 1);
    if (obj_carrying_pid_obj(Doctor_box, 46)) then begin
        removed_qty := obj_is_carrying_obj_pid(Doctor_box, 46);
        if (1 < removed_qty) then begin
            removed_qty := 1;
        end
        if (removed_qty > 0) then begin
            restock_obj := obj_carrying_pid_obj(Doctor_box, 46);
            if (obj_type(self_obj) == 1) then begin
                restock_obj := obj_carrying_pid_obj(Doctor_box, 46);
                if (critter_inven_obj(self_obj, INVEN_TYPE_WORN) == restock_obj) then begin
                    if (obj_item_subtype(critter_inven_obj(Doctor_box, INVEN_TYPE_WORN)) == 0) then begin
                        restock_obj := critter_inven_obj(Doctor_box, INVEN_TYPE_WORN);
                        debug_msg("armour pid == " + obj_pid(restock_obj));
                        rm_obj_from_inven(Doctor_box, restock_obj);
                        add_obj_to_inven(Doctor_box, restock_obj);
                    end
                end
                else begin
                    if ((critter_inven_obj(Doctor_box, INVEN_TYPE_LEFT_HAND) == restock_obj) or (critter_inven_obj(Doctor_box, INVEN_TYPE_RIGHT_HAND) == restock_obj)) then begin
                        inven_unwield(Doctor_box);
                    end
                end
            end
            restock_amt := rm_mult_objs_from_inven(Doctor_box, restock_obj, removed_qty);
            if (restock_amt == 0) then begin
                toRemove_qty := 1;
                while ((toRemove_qty > 0) and obj_carrying_pid_obj(Doctor_box, 46)) do begin
                    restock_obj := obj_carrying_pid_obj(Doctor_box, 46);
                    rm_obj_from_inven(Doctor_box, restock_obj);
                    destroy_object(restock_obj);
                    toRemove_qty := toRemove_qty - 1;
                end
            end
            else begin
                destroy_object(restock_obj);
            end
        end
        restock_obj := 0;
        restock_amt := 0;
        reed_bag := create_object(46, 0, 0);
        note := create_object_sid(535, 0, 0, SCRIPT_RH_RNOTE);
        restock_obj := create_object(101, 0, 0);
        add_mult_objs_to_inven(reed_bag, restock_obj, 1);
        restock_obj := create_object(295, 0, 0);
        add_mult_objs_to_inven(reed_bag, restock_obj, 1);
        restock_obj := create_object(317, 0, 0);
        add_mult_objs_to_inven(reed_bag, restock_obj, 1);
        add_obj_to_inven(dude_obj, reed_bag);
        add_obj_to_inven(reed_bag, note);
    end
    call Node999();
end

