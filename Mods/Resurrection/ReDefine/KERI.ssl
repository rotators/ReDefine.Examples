            end
            else begin
                destroy_object(restock_obj);
            end
        end
        restock_obj := 0;
        restock_amt := 0;
        restock_obj := create_object(74, 0, 0);
        add_mult_objs_to_inven(self_obj, restock_obj, 1);
        if (lPartyWornArmor == 1) then begin
            wield_obj_critter(dude_obj, obj_carrying_pid_obj(dude_obj, 74));
            art_change_fid_num(dude_obj, lFidArmor);
