procedure map_enter_p_proc
begin
    if (CUR_MAP_ALB_LOVA) then begin
        MH_elevator := self_obj;
    end
end

procedure spatial_p_proc
begin
    if (source_obj == dude_obj) then begin
        if (CUR_MAP_ALB_LOVA) then begin
            if ((global_var(GVAR_AQ_ENEMY_HUNTERS) == 1) or (global_var(GVAR_AQ_MUTANT_HUNTER) == 1) or (using_skill(dude_obj, 8) and is_success(roll_vs_skill(dude_obj, 8, -100)))) then begin
                elevator(-1);
            end
            else begin
                if (combat_is_initialized) then begin
                    script_overrides;
                    terminate_combat;
                    add_timer_event(ElevatorGuardian_obj, 1, 1);
                end
                else begin
                    script_overrides;
                    add_timer_event(ElevatorGuardian_obj, game_ticks(0), 1);
                end
            end
        end
        else begin
            elevator(-1);
        end
    end
end
