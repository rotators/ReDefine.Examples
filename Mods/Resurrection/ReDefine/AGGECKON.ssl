procedure combat_p_proc
begin
    variable LVar0 := 0;
    if (fixed_param == 2) then begin
        if (target_obj == dude_obj) then begin
            if (not(is_success(do_check(dude_obj, 6, -1)))) then begin
                LVar0 := random(1, get_pc_stat(PCSTAT_level) * 3);
