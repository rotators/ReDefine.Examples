procedure map_enter_p_proc
begin
    if (((global_var(GVAR_AQ_ALBUQ_EVENTS) bwand 65536) != 0) and (global_var(GVAR_AQE_ENEMY_EMPEROR) == 0) and ((global_var(GVAR_AQ_ALBUQ_EVENTS) bwand 32768) != 0)) then begin
        destroy_object(self_obj);
    end
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, 265);
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_PEASANTS);
    if (global_var(GVAR_C_QUEST_CHADWIC_WANT_SUZAN) == 3) then begin
        if (local_var(5) == 0) then begin
            lLocker := tile_contains_pid_obj(22889, 0, 188);
            if (obj_is_carrying_obj_pid(lLocker, 319) == 0) then begin
                restock_obj := create_object(319, 0, 0);
                add_mult_objs_to_inven(lLocker, restock_obj, 1);
            end
            set_local_var(5, game_time);
        end
        if ((game_time - local_var(5)) >= (24 * (60 * (60 * 10)) * 6)) then begin
            destroy_object(self_obj);
        end
        if (obj_is_visible_flag(self_obj) == 0) then begin
            set_obj_visibility(self_obj, 0);
        end
        rm_fixed_timer_event(self_obj, 1);
        add_timer_event(self_obj, game_ticks(random(10, 20)), 1);
    end
    else begin
        if (obj_is_visible_flag(self_obj) == 1) then begin
            set_obj_visibility(self_obj, 1);
        end
    end
    if (global_var(GVAR_C_QUEST_MURDERS) == 3) then begin
        destroy_object(self_obj);
    end
    if ((global_var(GVAR_C_RING_VICTORIES) bwand 1024) != 0) then begin
        destroy_object(self_obj);
    end
    if (global_var(GVAR_AQ_DOCTORS_COUNTER) >= 6) then begin
        destroy_object(self_obj);
    end
    if ((map_var(1) bwand 2) != 0) then begin
        destroy_object(self_obj);
    end
    if ((map_var(0) bwand 512) != 0) then begin
        destroy_object(self_obj);
    end
end

