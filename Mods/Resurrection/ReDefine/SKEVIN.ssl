    end
    if (fixed_param == 10) then begin
        float_msg(self_obj, mstr(301), FLOAT_MSG_YELLOW);
    end
    if (fixed_param == 11) then begin
        float_msg(self_obj, mstr(450), FLOAT_MSG_RED);
        call Node998();
        set_global_var(GVAR_ENEMY_SEDIT, 1);
    end
    if (fixed_param == 12) then begin
        float_msg(self_obj, mstr(303), FLOAT_MSG_RED);
    end
    if (fixed_param == 20) then begin
        if (fixed_param == 20) then begin
            sequence := sequence + 1;
        end
        if (sequence == 1) then begin
            game_ui_disable;
            add_timer_event(self_obj, game_ticks(1), 20);
        end
        if (sequence == 2) then begin
            gfade_out(600);
            set_global_var(GVAR_KARAVAN_ROUTES_COMMAND_BITS, global_var(GVAR_KARAVAN_ROUTES_COMMAND_BITS) bwor 1048576);
            play_sfx("MABROMAA");
