        end
        if (sequence == 3) then begin
            play_sfx("MABROMAA");
            add_timer_event(self_obj, game_ticks(1), 20);
        end
        if (sequence == 4) then begin
            game_ui_enable;
            add_timer_event(self_obj, game_ticks(0), 20);
        end
        if (sequence == 5) then begin
            if (to_corath == 1) then begin
                call Caravan_to_Corath2();
            end
            if (to_albuquerque == 1) then begin
                call Caravan_to_Albu2();
            end
        end
    end
end

