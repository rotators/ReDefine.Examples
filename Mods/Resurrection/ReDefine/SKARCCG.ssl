    end
end

procedure talk_p_proc
begin
    while (dialog_loop == 0) do begin
        talking_float := random(0, 13);
        loop_numbers := loop_numbers + 1;
        if ((talking_float >= 0) and (talking_float <= 4) or (loop_numbers == 7)) then begin
            float_msg(self_obj, message_str(SCRIPT_SKARCC, random(300, 304)), FLOAT_MSG_YELLOW);
            dialog_loop := 1;
        end
        if ((talking_float >= 5) and (talking_float <= 6) and (map_var(5) != 1)) then begin
            float_msg(self_obj, message_str(SCRIPT_SKARCC, random(311, 312)), FLOAT_MSG_YELLOW);
            dialog_loop := 1;
        end
        if ((talking_float >= 7) and (talking_float <= 9) and (map_var(5) == 1)) then begin
            if (dude_gender == 0) then begin
                float_msg(self_obj, message_str(SCRIPT_SKARCC, random(313, 315)), FLOAT_MSG_YELLOW);
            end
            if (dude_gender == 1) then begin
                float_msg(self_obj, message_str(SCRIPT_SKARCC, random(314, 316)), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
        if ((talking_float >= 10) and (talking_float <= 13) and (global_var(GVAR_CRIMSON_CARAVAN_LEAVE) == 1)) then begin
            if (dude_gender == 0) then begin
                float_msg(self_obj, message_str(SCRIPT_SKARCC, random(317, 320)), FLOAT_MSG_YELLOW);
            end
            if (dude_gender == 1) then begin
                float_msg(self_obj, message_str(SCRIPT_SKARCC, random(318, 321)), FLOAT_MSG_YELLOW);
            end
            dialog_loop := 1;
        end
    end
    dialog_loop := 0;
    loop_numbers := 0;
end

