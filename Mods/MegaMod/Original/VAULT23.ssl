procedure start;
procedure map_enter_p_proc;
procedure map_update_p_proc;


procedure start
begin
end

procedure map_enter_p_proc
begin
    if (map_var(1) == 0) then begin
        set_light_level(1);
    end
    if (map_var(1) == 1) then begin
        set_light_level(100);
    end
    if (global_var(767) < 2) then begin
        display_msg(message_str(1309, 890));
    end
end

procedure map_update_p_proc
begin
    if (map_var(1) == 0) then begin
        set_light_level(1);
    end
    if (map_var(1) == 1) then begin
        set_light_level(100);
    end
end

