        call node022();
    end
end

procedure critter_p_proc
begin
    if ((local_var(5) == 1) and obj_can_see_obj(self_obj, dude_obj)) then begin
        attack_complex(dude_obj, 0, 1, 0, 0, 30000, 0, 0);
    end
    if (obj_can_see_obj(self_obj, dude_obj) and (In_Timed_Event == 0)) then begin
        In_Timed_Event := 1;
        add_timer_event(self_obj, random(1, 5) * 10, 1);
    end
    if ((map_var(1) == 1) and (tile_distance_objs(self_obj, dude_obj) <= 15) and (local_var(4) != 2)) then begin
        call node020();
    end
    if (need_to_move) then begin
        if (tile_num(self_obj) != 21658) then begin
            if ((tile_num(self_obj) != 21658) and (21658 != 0)) then begin
                if ((dest_tile == 21658) and (step_tile < 0)) then begin
                    step_tile := step_tile + 1;
                end
                else begin
                    if (anim_busy(self_obj) == 0) then begin
                        dest_tile := 21658;
                        step_tile := 21658;
                        animate_move_obj_to_tile(self_obj, step_tile, 1);
                        debug_msg("move_to_point: self_tile == " + tile_num(self_obj));
                        while ((anim_busy(self_obj) == 0) and (tile_distance(tile_num(self_obj), 21658) >= tile_distance(step_tile, 21658))) do begin
                            step_tile := tile_num_in_direction(step_tile, rotation_to_tile(step_tile, tile_num(self_obj)), 5 % tile_distance(step_tile, tile_num(self_obj)));
                            debug_msg("move_to_point: step_tile == " + step_tile);
                            animate_move_obj_to_tile(self_obj, step_tile, 1);
                        end
                        if (anim_busy(self_obj) == 0) then begin
                            if (metarule3(106, tile_num_in_direction(tile_num(self_obj), has_trait(1, self_obj, 10), 1), elevation(self_obj), 0) != 0) then begin
                                if (anim_busy(self_obj) == 0) then begin
                                    global_temp := rotation_to_tile(tile_num_in_direction(tile_num(self_obj), rotation_to_tile(tile_num_in_direction(tile_num(self_obj), has_trait(1, self_obj, 10), 1), tile_num(self_obj)), 1), tile_num(self_obj));
                                    animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), global_temp, random(2, 5)), 0);
                                    global_temp := (global_temp + 1) % 6;
                                    while ((anim_busy(self_obj) == 0) and (global_temp != rotation_to_tile(tile_num_in_direction(tile_num(self_obj), rotation_to_tile(tile_num_in_direction(tile_num(self_obj), has_trait(1, self_obj, 10), 1), tile_num(self_obj)), 1), tile_num(self_obj)))) do begin
                                        debug_msg("flee loop: rot == " + global_temp);
                                        animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), global_temp, random(2, 5)), 0);
                                        global_temp := (global_temp + 1) % 6;
                                    end
                                end
                            end
                            else begin
                                step_tile := -10;
                            end
                        end
                    end
                    else begin
                        if (tile_distance(tile_num(self_obj), 21658) < tile_distance(dest_tile, 21658)) then begin
                            reg_anim_func(2, self_obj);
                        end
                    end
                end
            end
            else begin
                need_to_move := 0;
                destroy_object(self_obj);
            end
        end
    end
end

