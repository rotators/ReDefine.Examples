procedure timed_event_p_proc
begin
    if ((fixed_param == 1) and (combat_is_initialized == 0) and (in_dialog == 0) and (local_var(4) == 0) and (map_var(1) == 0) and (cur_map_index != 219)) then begin
        float_msg(self_obj, message_str(1312, random(104, 106)), 0);
        In_Timed_Event := 0;
    end
    if (fixed_param == 3) then begin
        call node021();
    end
