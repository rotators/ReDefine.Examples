                if (local_var(10) == 1) then begin
                    dtile := tile_num(dude_obj);
                    dlev := elevation(dude_obj);
                    mytile := tile_num(self_obj);
                    mylev := elevation(self_obj);
                    if ((tile_distance(mytile, 18335) < 7) and (mylev == 0)) then begin
                        call leave_player();
                    end
