procedure critter_p_proc
begin
    if ((local_var(4) != 0) or (global_var(GVAR_TOWN_REP_MODOC) <= -1000) and obj_can_see_obj(self_obj, dude_obj)) then begin
        if (critter_is_fleeing(self_obj)) then begin
            if (anim_busy(self_obj) == 0) then begin
                if ((tile_distance_objs(self_obj, dude_obj) < 8) or obj_can_see_obj(self_obj, dude_obj)) then begin
                    if (anim_busy(self_obj) == 0) then begin
                        GVar10 := rotation_to_tile(dude_tile, tile_num(self_obj));
                        animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), GVar10, random(3, 10)), 1);
                        GVar10 := (GVar10 + 1) % 6;
                        while ((anim_busy(self_obj) == 0) and (GVar10 != rotation_to_tile(dude_tile, tile_num(self_obj)))) do begin
                            debug_msg("flee loop: rot == " + GVar10);
                            animate_move_obj_to_tile(self_obj, tile_num_in_direction(tile_num(self_obj), GVar10, random(3, 10)), 1);
                            GVar10 := (GVar10 + 1) % 6;
                        end
                    end
                end
            end
        end
        else begin
            attack(dude_obj);
        end
    end
    else begin
        if (CUR_MAP_GSTFARM) then begin
            if (obj_is_visible_flag(self_obj)) then begin
                if (obj_can_see_obj(self_obj, ghost_farm_slag_obj)) then begin
                    attack(ghost_farm_slag_obj);
                end
                else begin
                    if (tile_distance_objs(self_obj, dude_obj) > 12) then begin
                        if (tile_distance_objs(self_obj, dude_obj) > (12 * 2)) then begin
                            animate_move_obj_to_tile(self_obj, tile_num_in_direction(dude_tile, (obj_get_rot(dude_obj) + 3) % 6, 9), 1);
                        end
                        else begin
                            animate_move_obj_to_tile(self_obj, tile_num_in_direction(dude_tile, (obj_get_rot(dude_obj) + 3) % 6, 9), 0);
                        end
                    end
                end
            end
        end
        else begin
            if (local_var(6) < ((map_var(0) - 1) * (map_var(0) >= 1))) then begin
                if (local_var(6) < 3) then begin
                    float_msg(self_obj, mstr(150 + local_var(6)), FLOAT_MSG_YELLOW);
                end
                set_local_var(6, local_var(6) + 1);
                if (local_var(6) >= 3) then begin
                    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_PEASANT_KAMAKAZI);
                    call Node998();
                end
            end
        end
    end
end

