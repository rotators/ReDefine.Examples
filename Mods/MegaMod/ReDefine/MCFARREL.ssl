procedure timed_event_p_proc
begin
    variable LVar0 := 0;
    if (fixed_param == 2) then begin
        float_msg(self_obj, " ", FLOAT_MSG_YELLOW);
    end
    else begin
        if (modoc_cornelius_obj != 0) then begin
            if (CUR_MAP_MODINN) then begin
                modoc_farrel_obj := self_obj;
            end
            if ((local_var(7) <= 0) or (local_var(7) > 3)) then begin
                set_local_var(7, 1);
            end
            float_msg(self_obj, mstr(500 + local_var(7)), 8 + ((2 - 8) * has_trait(TRAIT_PERK, dude_obj, PERK_empathy)));
            set_local_var(7, local_var(7) + 1);
            LVar0 := random(3, 5);
            add_timer_event(self_obj, game_ticks(LVar0) - 1, 2);
            add_timer_event(modoc_cornelius_obj, game_ticks(LVar0) + 1, 1);
        end
        else begin
            set_local_var(7, 0);
        end
    end
    return 0;
    LVar0 := 0;
end

