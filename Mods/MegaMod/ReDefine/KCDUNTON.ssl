procedure critter_p_proc
begin
    if (active) then begin
        if ((global_var(GVAR_KLAMATH_REP_ALIANCE) == 1) and obj_can_see_obj(self_obj, dude_obj)) then begin
            attack(dude_obj);
        end
        else begin
            if (CUR_MAP_KLADWTWN) then begin
                call checkarea();
                if ((area == 2) and (map_var(17) > 0) and obj_can_see_obj(self_obj, dude_obj)) then begin
                    set_map_var(17, 0);
                    float_msg(self_obj, message_str(SCRIPT_KCCDNTN, 600), FLOAT_MSG_RED);
                    set_global_var(GVAR_KLAMATH_REP_ALIANCE, 1);
                    attack(dude_obj);
                end
                else begin
                    needtomove := 0;
                    call gettarget();
                    if (needtomove == 1) then begin
                        if (local_var(5) > 0) then begin
                            animate_move_obj_to_tile(self_obj, local_var(5), 0);
                        end
                    end
                    else begin
                        if (lock_out_events == 0) then begin
                            lock_out_events := 1;
                            add_timer_event(self_obj, game_ticks(random(10, 15)), 0);
                        end
                    end
                end
            end
            else begin
                if ((global_var(GVAR_RUSTLE_FAIL) == 1) or (global_var(GVAR_RUSTLE_FAIL_VIOLENT) == 1) or ((map_var(5) >= map_var(19)) and (global_var(GVAR_RUSTLE_SUCCESS) != 1))) then begin
                    set_global_var(GVAR_RUSTLE_OVER, 1);
                    dest := tile_num_in_direction(tile_num(self_obj), 5, 12);
                    animate_move_obj_to_tile(self_obj, dest, 1);
                    add_timer_event(self_obj, game_ticks(2), 1);
                end
            end
        end
    end
end

procedure timed_event_p_proc
begin
    if (active) then begin
        if (fixed_param == 0) then begin
            lock_out_events := 0;
            new_tile := tile_num_in_direction(tile_num(self_obj), random(0, 5), random(1, 3));
            call checktile();
            if (tile != 0) then begin
                animate_move_obj_to_tile(self_obj, new_tile, 0);
            end
        end
        else begin
            if (fixed_param == 1) then begin
                call scamper();
            end
        end
    end
end

