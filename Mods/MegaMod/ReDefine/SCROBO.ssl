procedure Heal_Robo(variable arg0)
begin
    if (local_var(12) < has_skill(source_obj, SKILL_SPEECH)) then begin
        if (is_success(roll_vs_skill(source_obj, 13, arg0))) then begin
            critter_heal(self_obj, random(10, 40));
            set_local_var(12, local_var(12) + 50);
        end
        if (get_critter_stat(self_obj, STAT_current_hp) != get_critter_stat(self_obj, STAT_max_hp)) then begin
            display_mstr(132);
        end
        else begin
            display_mstr(133);
        end
    end
    else begin
        display_mstr(131);
    end
end

