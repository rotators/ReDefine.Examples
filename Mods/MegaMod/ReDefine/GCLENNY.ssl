                                else begin
                                    if ((local_var(10) != 0) == 0) then begin
                                        if (waiting != 1) then begin
                                            if (((local_var(10) != 0) == 0) and ((global_var(GVAR_PARTY_NO_FOLLOW) != 0) == 0)) then begin
                                                if (local_var(11) == 0) then begin
