procedure map_enter_p_proc
begin
    Only_Once := 0;
    if (not(is_loading_game)) then begin
        if (map_first_run) then begin
            set_local_var(7, tile_num(self_obj));
            if (obj_is_carrying_obj_pid(self_obj, 299) < 1) then begin
                restock_obj := create_object(299, 0, 0);
                add_mult_objs_to_inven(self_obj, restock_obj, 1 - obj_is_carrying_obj_pid(self_obj, 299));
            end
            else begin
                if (obj_is_carrying_obj_pid(self_obj, 299) > 1) then begin
                    removed_qty := obj_is_carrying_obj_pid(self_obj, 299);
                    if ((obj_is_carrying_obj_pid(self_obj, 299) - 1) < removed_qty) then begin
                        removed_qty := obj_is_carrying_obj_pid(self_obj, 299) - 1;
                    end
                    if (removed_qty > 0) then begin
                        restock_obj := obj_carrying_pid_obj(self_obj, 299);
                        if (obj_type(self_obj) == 1) then begin
                            restock_obj := obj_carrying_pid_obj(self_obj, 299);
                            if (critter_inven_obj(self_obj, 0) == restock_obj) then begin
                                if (obj_item_subtype(critter_inven_obj(self_obj, 0)) == 0) then begin
                                    restock_obj := critter_inven_obj(self_obj, 0);
                                    debug_msg("armour pid == " + obj_pid(restock_obj));
                                    rm_obj_from_inven(self_obj, restock_obj);
                                    add_obj_to_inven(self_obj, restock_obj);
                                end
                            end
                            else begin
                                if ((critter_inven_obj(self_obj, 2) == restock_obj) or (critter_inven_obj(self_obj, 1) == restock_obj)) then begin
                                    inven_unwield(self_obj);
                                end
                            end
                        end
                        restock_amt := rm_mult_objs_from_inven(self_obj, restock_obj, removed_qty);
                        destroy_object(restock_obj);
                    end
                    restock_obj := 0;
                    restock_amt := 0;
                end
                else begin
                    restock_obj := 0;
                end
