procedure critter_p_proc
begin
    if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
        set_local_var(5, 1);
        attack(dude_obj);
    end
    if (((global_var(GVAR_SAN_FRAN_FLAGS) bwand 1) != 0) and obj_can_see_obj(self_obj, dude_obj)) then begin
        call talk_p_proc();
    end
end

procedure pickup_p_proc
begin
    if (source_obj == dude_obj) then begin
        if (pickupWarning < 2) then begin
            call Node002();
        end
        else begin
            set_local_var(5, 2);
        end
        pickupWarning := pickupWarning + 1;
    end
end

