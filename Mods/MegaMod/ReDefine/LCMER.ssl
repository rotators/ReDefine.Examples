procedure critter_p_proc
begin
    if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
        set_local_var(5, 1);
        attack(dude_obj);
    end
    if ((global_var(GVAR_LYDIA_WAITING_FOR_LEON) > 0) and obj_can_see_obj(self_obj, dude_obj)) then begin
        attack(dude_obj);
        float_msg(self_obj, mstr(random(103, 106)), FLOAT_MSG_RED);
    end
    if ((map_var(4) == 15) or (map_var(4) == 47) and (dude_perception > 4)) then begin
        display_mstr(107);
        add_timer_event(self_obj, 30, 1);
        set_map_var(4, map_var(4) bwor 16);
    end
end

procedure pickup_p_proc
begin
    if (source_obj == dude_obj) then begin
        set_local_var(5, 2);
    end
end

procedure talk_p_proc
begin
    if (global_var(GVAR_DUDE_IS_SCRB) == 3) then begin
        call Node016();
    end
    else begin
        if (map_var(2) > 0) then begin
            call Node009();
        end
        else begin
            call Node002();
        end
    end
    set_local_var(4, 1);
end

procedure destroy_p_proc
begin
    if (map_var(2) == 5) then begin
        set_map_var(2, 6);
    end
end

procedure look_at_p_proc
begin
    script_overrides;
    if (local_var(4) == 0) then begin
        display_mstr(100);
    end
