    Reply(600);
    if (map_var(4) == 63) then begin
        giq_option(4, NAME, 610, Node010, NEUTRAL_REACTION);
        if ((map_var(3) bwand 1) != 0) then begin
            giq_option(4, NAME, 752, Node012, GOOD_REACTION);
        end
        if ((map_var(3) bwand 2) != 0) then begin
