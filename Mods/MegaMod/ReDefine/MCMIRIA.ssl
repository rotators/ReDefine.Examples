procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        if (not(combat_is_initialized) and obj_is_visible_flag(self_obj)) then begin
            if (party_member_obj(16777380) != 0) then begin
                if (get_critter_stat(self_obj, STAT_current_rad)) then begin
                    float_msg(self_obj, mstr(random(5000, 5001)), FLOAT_MSG_YELLOW);
                end
                else begin
                    if (get_poison(self_obj)) then begin
                        float_msg(self_obj, mstr(random(5002, 5003)), FLOAT_MSG_YELLOW);
                    end
                    else begin
                        if (critter_state(party_member_obj(16777380)) bwand (4 bwor 8 bwor 16 bwor 32)) then begin
                            float_msg(self_obj, mstr(random(5004, 5005)), FLOAT_MSG_YELLOW);
                        end
                        else begin
                            if (get_critter_stat(self_obj, STAT_current_hp) <= (get_critter_stat(self_obj, STAT_max_hp) * 3 / 4)) then begin
                                float_msg(self_obj, mstr(random(5006, 5007)), FLOAT_MSG_YELLOW);
                            end
                            else begin
                                float_msg(self_obj, mstr(random(5500, 5520)), FLOAT_MSG_YELLOW);
                            end
                        end
                    end
                end
            end
        end
        add_timer_event(self_obj, game_ticks(random(40, 50)), 1);
    end
end

