                                        LVar0 := 1800;
                                    end
                                    else begin
                                        if (get_pc_stat(PCSTAT_level) > 29) then begin
                                            LVar0 := 2000;
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    item_caps_adjust(self_obj, LVar0);
end
