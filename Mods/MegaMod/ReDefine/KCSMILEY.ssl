procedure critter_p_proc
begin
    if (active) then begin
        if ((cur_map_index != MAP_KLATOXCV) and (global_var(GVAR_KLAMATH_REP_UNION) == -1)) then begin
            attack(dude_obj);
        end
        if ((local_var(5) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
            set_local_var(5, 1);
            attack(dude_obj);
        end
        else begin
            if ((global_var(GVAR_KLAMATH_REP_UNION) <= -1) and obj_can_see_obj(self_obj, dude_obj)) then begin
                attack(dude_obj);
            end
            else begin
                if (local_var(10) == 1) then begin
                    dtile := dude_tile;
                    dlev := dude_elevation;
                    mytile := tile_num(self_obj);
                    mylev := elevation(self_obj);
                    if ((tile_distance(mytile, 18335) < 7) and (mylev == 0)) then begin
                        call leave_player();
                    end
                    else begin
                        if (mylev != dlev) then begin
                            newtile := tile_num_in_direction(dtile, random(0, 5), 1);
                            critter_attempt_placement(self_obj, newtile, dlev);
                        end
                        else begin
                            if (tile_distance(mytile, dtile) > 5) then begin
                                newtile := tile_num_in_direction(dtile, random(0, 5), 1);
                                animate_move_obj_to_tile(self_obj, newtile, 1);
                            end
                        end
                    end
                end
            end
        end
    end
end

