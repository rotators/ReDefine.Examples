procedure use_p_proc
begin
    if (local_var(7) == 0) then begin
        if (dude_strength >= 10) then begin
            move_to(self_obj, 19873, 1);
            move_to(dude_obj, 19876, 1);
            set_local_var(7, 1);
            display_mstr(104);
        end
        else begin
            if (dude_strength < 10) then begin
                display_mstr(105);
            end
        end
    end
    else begin
        display_mstr(106);
    end
end

