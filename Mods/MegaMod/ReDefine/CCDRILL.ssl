            end
        end
        if (obj_can_see_obj(self_obj, dude_obj)) then begin
            if (local_var(5) == 2) then begin
                set_local_var(5, 1);
                attack(dude_obj);
            end
            else begin
                if ((global_var(GVAR_NAVARRO_BASE_ALERT) > 0) and (local_var(10) == 0)) then begin
                    set_local_var(10, 1);
                    dialogue_system_enter;
                end
                else begin
                    if ((party_member_count(0) - 1 - (party_member_obj(16777687) != 0)) >= 1) then begin
                        call Node021();
                    end
                    else begin
                        if ((tile_distance(14930, dude_tile) > 10) and (local_var(11) == 1)) then begin
                            dialogue_system_enter;
                        end
                    end
                end
            end
        end
    end
    if (need_run_to_frank == 1) then begin
        animate_move_obj_to_tile(self_obj, 25496, 1);
        if (tile_num(self_obj) == 25496) then begin
            anim(self_obj, 1000, 4);
            need_run_to_frank := 0;
        end
    end
    if (need_run_to_frank == 2) then begin
        need_run_to_frank := 0;
        animate_move_obj_to_tile(self_obj, self_prev_tile, 0);
