                if (tile_distance_objs(self_obj, dude_obj) > (3 * local_var(8) / 2)) then begin
                    if (anim_busy(self_obj) == 0) then begin
                        dest_tile := tile_num_in_direction(tile_num_in_direction(dude_tile, rotation_to_tile(dude_tile, tile_num(self_obj)), local_var(8)), random(0, 5), random(0, 2));
                        if (tile_distance_objs(self_obj, dude_obj) > (3 * local_var(8) / 2 * 2)) then begin
                            animate_move_obj_to_tile(self_obj, dest_tile, 1);
                        end
                        else begin
                            animate_move_obj_to_tile(self_obj, dest_tile, 0);
                        end
                    end
                    else begin
                        if (tile_distance(tile_num(self_obj), dude_tile) < tile_distance(tile_num(self_obj), dest_tile)) then begin
                            reg_anim_func(2, self_obj);
                        end
                    end
