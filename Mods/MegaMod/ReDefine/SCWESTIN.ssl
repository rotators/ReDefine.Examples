procedure getDisk
begin
    variable LVar0 := 0;
    LVar0 := obj_carrying_pid_obj(dude_obj, 447);
    if (LVar0) then begin
        set_global_var(GVAR_VAULT_DELIVER_HOLODISK, 3);
        rm_obj_from_inven(dude_obj, LVar0);
        destroy_object(LVar0);
    end
end

procedure giveDisk
begin
    variable LVar0 := 0;
    LVar0 := create_object_sid(338, tile_num(self_obj), elevation(self_obj), SCRIPT_SIWESHOL);
    if (LVar0) then begin
        set_global_var(GVAR_NCR_LYNETTE_HOLO_QST, 1);
        add_obj_to_inven(dude_obj, LVar0);
    end
end
