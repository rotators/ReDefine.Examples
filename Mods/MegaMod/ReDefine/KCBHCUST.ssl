        set_local_var(4, 1);
        attack(dude_obj);
    end
    if ((global_var(GVAR_KLAMATH_REP_UNION) <= -1) and obj_can_see_obj(self_obj, dude_obj)) then begin
        attack(dude_obj);
    end
    if (map_var(6) == 1) then begin
        set_map_var(6, 0);
        call thanks();
    end
    else begin
        if ((random(0, 200) == 1) and (In_Timed_Event == 0)) then begin
