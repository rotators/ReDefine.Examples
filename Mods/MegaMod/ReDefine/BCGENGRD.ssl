procedure map_enter_p_proc
begin
    Only_Once := 0;
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_TEAM_NUM, 119);
    critter_add_trait(self_obj, TRAIT_OBJECT, OBJECT_AI_PACKET, AI_KHAN);
    add_timer_event(self_obj, game_ticks(random(30, 50)), 1);
    guardNum := i_Guard_Count;
    i_Guard_Count := i_Guard_Count + 1;
    guardFloatNum := guardNum;
    if (guardFloatNum >= 5) then begin
        while (guardFloatNum >= 5) do begin
            guardFloatNum := guardFloatNum - 5;
        end
    end
end

