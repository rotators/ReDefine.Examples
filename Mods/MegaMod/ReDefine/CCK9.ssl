            giq_option(4, NAME, 3015, Node1011, NEUTRAL_REACTION);
            giq_option(4, NAME, g_mstr(10007), Node999, NEUTRAL_REACTION);
            giq_option(-3, NAME, g_mstr(random(300, 303)), Node999, NEUTRAL_REACTION);
        end
    end
