            end
        end
    end
    else begin
        if ((local_var(0) == 2) and obj_can_see_obj(self_obj, dude_obj)) then begin
            set_local_var(0, 1);
            attack(dude_obj);
        end
        else begin
            if (party_member_obj(16777687) != 0) then begin
                if ((party_member_obj(16777687) != 0) and ((local_var(5) != 0) == 0)) then begin
