procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        if (combat_is_initialized == 0) then begin
            if (obj_can_see_obj(self_obj, dude_obj)) then begin
                should_attack := 1;
                if (new_reno_eldridge_obj != 0) then begin
                    if (obj_can_see_obj(new_reno_eldridge_obj, dude_obj)) then begin
                        should_attack := 0;
                    end
                    else begin
                        if ((game_time_hour >= 600) and (game_time_hour <= 2100)) then begin
                            should_attack := 0;
                        end
                    end
                end
                if (tile_in_tile_rect(22334, 22306, 29534, 29506, dude_tile) and (CUR_MAP_NEWR3) and ((elevation(dude_obj) == 0) or (-1 == 0))) then begin
                    if (should_attack) then begin
                        if (tile_distance_objs(self_obj, dude_obj) > 1) then begin
                            animate_move_obj_to_tile(self_obj, tile_num_in_direction(dude_tile, rotation_to_tile(dude_tile, tile_num(self_obj)), 1), 1);
                        end
                        else begin
                            if (rotation_to_tile(tile_num(self_obj), dude_tile) != obj_get_rot(self_obj)) then begin
                                if (anim_busy(self_obj) == 0) then begin
                                    anim(self_obj, 1000, rotation_to_tile(tile_num(self_obj), dude_tile));
                                end
                            end
                        end
                        if (anim_busy(self_obj)) then begin
                            if (tile_distance_objs(self_obj, dude_obj) < 5) then begin
                                attack(dude_obj);
                            end
                        end
                    end
                    else begin
                        if (random(0, 2) != 2) then begin
                            if (new_reno_eldridge_obj != 0) then begin
                                last_timed_obj := new_reno_eldridge_obj;
                                last_timed_param := last_timed_param;
                                metarule3(100, new_reno_eldridge_obj, 13, 0);
                                add_timer_event(new_reno_eldridge_obj, 5, 13);
                            end
                            play_sfx("maddogaq");
                        end
                        new_reno_barking_dog_obj := self_obj;
                        anim(self_obj, 1000, rotation_to_tile(tile_num(self_obj), dude_tile));
                        float_msg(self_obj, message_str(SCRIPT_MCATKDOG, random(205, 206)), FLOAT_MSG_YELLOW);
                    end
                end
                else begin
                    if ((global_var(GVAR_NEW_RENO_FLAG_3) bwand 134217728) != 0) then begin
                        if (random(0, 2) != 2) then begin
                            if (new_reno_eldridge_obj != 0) then begin
                                last_timed_obj := new_reno_eldridge_obj;
                                last_timed_param := last_timed_param;
                                metarule3(100, new_reno_eldridge_obj, 13, 0);
                                add_timer_event(new_reno_eldridge_obj, 5, 13);
                            end
                            play_sfx("maddogaq");
                        end
                        new_reno_barking_dog_obj := self_obj;
                        anim(self_obj, 1000, rotation_to_tile(tile_num(self_obj), dude_tile));
                        float_msg(self_obj, message_str(SCRIPT_MCATKDOG, random(205, 206)), FLOAT_MSG_YELLOW);
                    end
                end
            end
        end
        else begin
            if (random(1, 10) == 4) then begin
                if (anim_busy(self_obj) == 0) then begin
                    temp_move_tile := tile_num_in_direction(tile_num(self_obj), random(0, 5), random(1, 3));
                    if (tile_in_tile_rect(22334, 22306, 29534, 29506, temp_move_tile)) then begin
                        if (tile_in_tile_rect(26125, 26115, 27925, 27915, temp_move_tile) == 0) then begin
                            set_local_var(1, temp_move_tile);
                        end
                    end
                    animate_move_obj_to_tile(self_obj, local_var(1), 0);
                end
            end
        end
        last_timed_obj := self_obj;
        last_timed_param := last_timed_param;
        metarule3(100, self_obj, 1, 0);
        add_timer_event(self_obj, game_ticks(3) + random(0, game_ticks(6 - 3)), 1);
    end
    else begin
        if (fixed_param == 8) then begin
            attack(dude_obj);
        end
    end
end

