procedure talk_p_proc
begin
    if ((global_var(GVAR_MARY_ANN_SON_QUEST) < 3) or (global_var(GVAR_MARY_ANN_SON_QUEST) > 4)) then begin
        float_msg(self_obj, mstr(400), FLOAT_MSG_YELLOW);
    end
    else begin
        float_msg(self_obj, mstr(401), FLOAT_MSG_YELLOW);
    end
end

procedure critter_p_proc
begin
    variable LVar0 := 0;
    if ((global_var(GVAR_MARY_ANN_SON_QUEST) < 3) or (global_var(GVAR_MARY_ANN_SON_QUEST) > 4)) then begin
        if (obj_can_see_obj(self_obj, dude_obj)) then begin
            if (dude_gender == 0) then begin
                float_msg(self_obj, mstr(random(200, 207)), FLOAT_MSG_YELLOW);
            end
            else begin
                float_msg(self_obj, mstr(random(300, 307)), FLOAT_MSG_YELLOW);
            end
            attack(dude_obj);
        end
    end
    return 0;
    LVar0 := 0;
end

