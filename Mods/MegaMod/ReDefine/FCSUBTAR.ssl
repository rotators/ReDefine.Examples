procedure use_p_proc
begin
    if (global_var(GVAR_SAN_FRAN_SUB) < 5) then begin
        script_overrides;
        display_mstr(200);
    end
end

procedure look_at_p_proc
begin
    script_overrides;
    display_mstr(100);
end

procedure description_p_proc
begin
    script_overrides;
    display_mstr(101);
end

procedure timed_event_p_proc
begin
    if (fixed_param == 1) then begin
        float_msg(self_obj, mstr(random(212, 213)), FLOAT_MSG_YELLOW);
        add_timer_event(self_obj, game_ticks(random(25, 35)), 1);
    end
end

procedure map_enter_p_proc
begin
    add_timer_event(self_obj, game_ticks(random(5, 10)), 1);
end

procedure use_skill_on_p_proc
begin
    if (global_var(GVAR_SAN_FRAN_SUB) < 5) then begin
        if (has_skill(dude_obj, SKILL_SCIENCE) > 100) then begin
            script_overrides;
            display_mstr(210);
            set_global_var(GVAR_SAN_FRAN_SUB, 5);
        end
        else begin
            script_overrides;
            display_mstr(211);
        end
    end
end

