        create_object(537, 27106, 0);
    end
    if ((global_var(GVAR_AHELDER_PROVE_TALK_FLAG) == 3) and (global_var(GVAR_BALTHAS_TEACH) == 1)) then begin
        set_global_var(GVAR_BALTHAS_TEACH, 1);
