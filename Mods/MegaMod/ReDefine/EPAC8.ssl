procedure map_enter_p_proc
begin
    variable LVar0 := 0;
    if (map_first_run) then begin
        LVar0 := create_object(365, 0, 0);
        add_mult_objs_to_inven(self_obj, LVar0, 10);
    end
    return 0;
    LVar0 := 0;
end

