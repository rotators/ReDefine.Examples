            while (global_var(GVAR_CAR_PLACED_TILE) != -1) do begin
                set_global_var(GVAR_CAR_PLACED_TILE, -1);
            end
            while (global_var(GVAR_CAR_CURRENT_TOWN) != -1) do begin
